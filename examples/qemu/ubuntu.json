{
  "builders":
  [
    {
      "name": "ubuntu_test",
      "vm_name": "my_ubuntu",
      "type": "qemu",
      "iso_checksum_type": "sha256",
      "iso_checksum_url": "http://releases.ubuntu.com/16.04.4/SHA256SUMS",
      "iso_urls": [ "http://releases.ubuntu.com/16.04.4/ubuntu-16.04.4-server-amd64.iso" ],
      "ssh_username": "vagrant",
      "ssh_password": "vagrant",
      "ssh_wait_timeout": "60m",
      "shutdown_command": "echo 'vagrant' | sudo -S shutdown -P now",

      "accelerator": "kvm",
      "qemuargs": [[ "-m", "4096" ], [ "-smp", "2" ]],
      "boot_wait": "2s",
      "boot_command":
      [
        "<enter><wait><f6><esc><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs>",
        "<bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs>",
        "<bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs>",
        "<bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs><bs>",
        "/install/vmlinuz ",
        "initrd=/install/initrd.gz ",
        "url=http://{{ .HTTPIP }}:{{ .HTTPPort }}/preseed.cfg ",
        "auto=true ",
        "net.ifnames=0 ",
        "hostname=localhost ",
        "<enter>"
      ],
      "disk_size": "51200",
      "http_directory": "."
    }
  ]
}
